{
    "browser_action": {
        "default_title": "Tab Organizer",
        "default_popup": "tabs.html",
        "default_icon": "icons/tabs_167113.png"
    },
    "description": "A list of methods you can perform on a tab.",
    "homepage_url": "https://github.com/ckim115/Firefox-Tab-Organizer",
    "manifest_version": 2,
    "name": "Tabs Organizer",
    "icons": {
        "48": "icons/tabs_167113.png"
    },

    "permissions": [
        "<all_urls>",
        "bookmarks",
        "tabs",
        "storage"
    ],
    "version": "1.0"
}
  