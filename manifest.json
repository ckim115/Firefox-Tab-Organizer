{
    "browser_action": {
        "default_title": "Tab Organizer",
        "default_popup": "tabs.html",
        "default_icon": "icons/tabs_167113.png"
    },
    "description": "A list of methods you can perform on a tab.",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/main/tabs-tabs-tabs",
    "manifest_version": 2,
    "name": "Tabs Organizer",
    "icons": {
        "48": "icons/tabs_167113.png",
        "64": "icons/alert-icon-1562.png"
    },

    "permissions": [
        "<all_urls>",
        "bookmarks",
        "tabs",
        "notifications"
    ],
    "version": "1.0"
}
  